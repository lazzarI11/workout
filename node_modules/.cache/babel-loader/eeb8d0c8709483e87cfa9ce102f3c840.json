A,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA,EAAA;AAAa,MAAA,IAAA,CAAA,KAAA,CAAK,gBAAL,EAAK;AAAA,QAAA,GAAA,EAAA,KAAA,CAA+B,GAA/B;AAAkC,QAAA,GAAA,EAAO,KAAA,CAAA;AAAzC,OAAL;;AAAkE,MAAA,IAAA,CAAA,KAAA,CAAK,UAAL,EAAK,IAAL;;AAAmC,MAAA,QAAA,CAAA,KAAA;KAAlH;AAOC,GATT,EAOC,WAPD,C,qCA+BI,EApBI,KAAA,CAAA,O,kBADR,mBAAA,CASS,QATT,EASS;UAAA;AAPN,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA,EAAA;AAAa,MAAA,IAAA,CAAA,KAAA,CAAK,QAAL,EAAK;AAAA,QAAA,GAAA,EAAA,KAAA,CAAuB;AAAvB,OAAL;;AAA8C,MAAA,IAAA,CAAA,KAAA,CAAK,UAAL,EAAK,KAAL;;YAAyC,O,GAAO,K;KAA3G;AAOC,GATT,EAOC,QAPD,C,qCAqBI,EAVI,KAAA,CAAA,O,kBADR,mBAAA,CAUS,QAVT,EAUS;UAAA;AARN,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA,EAAA;AAAa,MAAA,IAAA,CAAA,KAAA,CAAK,UAAL,EAAK;AAAA,QAAA,GAAA,EAAA,KAAA,CAAyB;AAAzB,OAAL;;AAAgD,MAAA,IAAA,CAAA,KAAA,CAAK,UAAL,EAAK,KAAL;;YAAyC,O,GAAO,K;AAAoB,MAAA,QAAA,CAAA,UAAA;KAAjI;AAQC,GAVT,EAQC,UARD,C,qCAWI,CAtCN,CAuCI,CA9EN,C","sourcesContent":["<template>\n  <!-- <Dialog :show=\"showDialog\"\n          :cancel=\"cancel\"\n          :confirm=\"$emit('deleteEx',{day:this.day})\"\n          title=\"Delete a day?\"\n          description=\"Are you sure you want to delete this day?\" /> -->\n\n  <div class=\"outerselect\">\n    <div v-if=\"!isAdded\" class=\"exercises\">\n      <div class=\"exCatSelect\">\n        <h2>Day {{ day }}</h2>\n\n        <div class=\"exercise\">\n          <span>Rest Day</span>\n          <input\n            type=\"checkbox\"\n            :value=\"true\"\n            v-model=\"isRestDay\"\n            @change=\"\n              restDaySelected();\n              $emit('emitSelectedEx', { day: this.day, arr: exerc });\n              $emit('dayAdded', true);\n            \"\n          />\n          <br />\n        </div>\n        <br />\n        <select\n          v-model=\"catValue\"\n          id=\"selectCat\"\n          @change=\"$emit('emitCategory', catValue)\"\n        >\n          <option value=\"\">Category</option>\n          <option value=\"3\">Chest</option>\n          <option value=\"1\">Abs</option>\n          <option value=\"2\">Legs</option>\n          <option value=\"4\">Arms</option>\n        </select>\n      </div>\n\n      <div class=\"exercise\" v-for=\"exercise in exercises\" :key=\"exercise.ex_id\">\n        {{ exercise.ex_name }}\n        <input type=\"checkbox\" :value=\"exercise\" v-model=\"exerc\" />\n      </div>\n    </div>\n\n    <div class=\"selected\">\n      <h4>Selected:</h4>\n\n      <div v-for=\"e in exerc\" :key=\"e.ex_id\">\n        {{ e.ex_name }}\n      </div>\n\n      <button\n        v-if=\"!isAdded\"\n        @click=\"\n          $emit('emitSelectedEx', { day: this.day, arr: exerc });\n          $emit('dayAdded', true);\n          added();\n        \"\n      >\n        Add day\n      </button>\n      <button\n        v-if=\"isAdded\"\n        @click=\"\n          $emit('editEx', { day: this.day });\n          $emit('dayAdded', false);\n          this.isAdded = false;\n        \"\n      >\n        Edit\n      </button>\n      <button\n        v-if=\"isAdded\"\n        @click=\"\n          $emit('deleteEx', { day: this.day });\n          $emit('dayAdded', false);\n          this.isAdded = false;\n          emptyExerc();\n        \"\n      >\n        Delete\n      </button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Dialog from \"./Dialog\";\nexport default {\n  components: { Dialog },\n\n  props: {\n    day: Number,\n    exercises: Array,\n    id: String,\n  },\n  emits: [\"emitCategory\", \"emitSelectedEx\", \"deleteEx\", \"editEx\", \"dayAdded\"],\n\n  data() {\n    return {\n      catValue: \"\",\n      exerc: [],\n      isAdded: false,\n      showDialog: false,\n      id: this.id,\n      day: this.day,\n      isRestDay: false,\n    };\n  },\n\n  computed: {\n    exSelected() {\n      return this.exerc.length;\n    },\n  },\n\n  watch: {\n    exSelected(selectedEx) {\n      if (selectedEx > 4) {\n        alert(\"You can select maximum 4 exercises per day!\");\n        this.exerc.pop();\n      }\n    },\n  },\n\n  methods: {\n    restDaySelected() {\n      if (this.isRestDay) {\n        this.exerc = [{ ex_id: 32, ex_name: \"REST DAY\" }];\n        this.isAdded = true;\n        this.isRestDay = false;\n        console.log(this.exerc);\n      }\n    },\n\n    added() {\n      if (this.exerc.length === 0) {\n        alert(\"You must select exercise!\");\n      } else {\n        this.isAdded = true;\n      }\n    },\n    emptyExerc() {\n      this.exerc = [];\n    },\n\n    cancel() {\n      console.log(\"cancel\");\n      this.showDialog = false;\n    },\n  },\n};\n</script>\n\n<style scoped>\ninput[type=\"checkbox\" i] {\n  margin: 10px 5px 1px 5px;\n  padding: 12px 0px 12px 0px;\n}\n.outerselect {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n.days {\n  display: flex;\n}\n.exercises {\n  /* max-height: 500px; */\n  width: 200px;\n  border: 3px solid #cd9f46;\n  display: flex;\n  flex-direction: column;\n}\n.exCatSelect {\n  margin: 10px;\n}\n.exCatSelect h2{\n  text-align: center;\n}\n.exCatSelect select{\n  display: block;\n  position: relative;\n  margin: auto;\n  margin-bottom: 0px;\n}\n\n.exercise {\n  min-height: 30px;\n  padding: 5px;\n  display: inline;\n  align-items: center;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  border-bottom: 1px solid #cd9f46;\n}\n\n.selected {\n  max-height: 500px;\n  width: 200px;\n  display: flex;\n  flex-direction: column;\n}\n\nbutton {\n  \n  justify-content: center;\n  margin: auto;\n  margin-top: 10px;\n  border-radius: 7px;\n  width: 100px;\n  height: 30px;\n  font-size: 15px;\n  font-weight: bold;\n  border: 3px solid #cd9f46;\n}\n</style>"],"sourceRoot":""}]}