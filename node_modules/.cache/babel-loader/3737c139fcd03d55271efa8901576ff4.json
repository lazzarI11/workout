      <h2>And your current workout level is {{usrLvl}} </h2>\n                 <h2> {{message}}</h2>\n\n</div> -->\n\n    <div class=\"profile\">\n      <!-- <img src=\"https://www.bootdey.com/img/Content/avatar/avatar7.png\" alt=\"profile\" id=\"pic\"> -->\n      <img\n        :src=\"\n          'http://576e122.e2.mars-hosting.com/api/plan-vezbanja/workout-pictures/' +\n          this.user.usr_id\n        \"\n        alt=\"profile\"\n        id=\"pic\"\n      />\n\n      <p>{{ this.user.usr_name }}</p>\n\n      <p>Workout status:</p>\n\n      <p>{{ message }}</p>\n\n      <button @click=\"move()\">Checkout</button>\n    </div>\n\n    <div class=\"usrInfo\">\n      <div class=\"field\">\n        <p>Full name:{{ this.user.usr_name }}</p>\n      </div>\n\n      <div class=\"field\">\n        <p>E-mail: {{ this.user.usr_email }}</p>\n      </div>\n\n      <div class=\"field\">\n        <p>Workout Place: {{ usrWop }}</p>\n      </div>\n\n      <div class=\"field\">\n        <p>Workout level: {{ usrLvl }}</p>\n      </div>\n\n      <div class=\"field\">\n        <p>Workout goal: {{ goal }}</p>\n      </div>\n\n      <button @click=\"metoda()\">Edit workout</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { mapState, mapActions } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      usrLvl: \"\",\n      usrWop: \"\",\n      message: \"\",\n      goal: \"\",\n    };\n  },\n  mounted() {\n    this.getUserData();\n  },\n\n  computed: {\n    ...mapState([\"isLoggedIn\", \"user\", \"sid\", \"isAdmin\", \"usrRequest\"]),\n  },\n  methods: {\n    ...mapActions([\"userStore\", \"changeUsrRequest\"]),\n\n    move() {\n      this.$router.push(\"/myplan\");\n    },\n\n    async getUserData() {\n      const params = {\n        usrId: this.user.usr_id,\n      };\n      try {\n        const res = await axios.post(\n          \"http://576e122.e2.mars-hosting.com/api/plan-vezbanja/usr-workout-info\",\n          params\n        );\n        console.log(res);\n        this.usrLvl = res.data.wkData.lvl_name;\n        this.usrWop = res.data.wkData.wop_name;\n        this.goal = res.data.wkData.subcat_name;\n        this.message = res.data.message;\n      } catch (eror) {\n        console.log(eror);\n      }\n    },\n\n    async editWorkout() {\n      const params = {\n        usrId: this.user.usr_id,\n      };\n      try {\n        const res = await axios.post(\n          \"http://576e122.e2.mars-hosting.com/api/plan-vezbanja/workout-editPlan\",\n          params\n        );\n      } catch (eror) {\n        console.log(eror);\n      }\n    },\n\n    metoda() {\n      if (\n        window.confirm(\"Your previous workout will be deleted,are you sure?\")\n      ) {\n        this.editWorkout();\n        this.changeUsrRequest(false);\n        this.$router.push(\"/workout\");\n      } else {\n        console.log(\"ok\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.wrapper {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  width: 90%;\n  height: auto;\n  margin: auto;\n  padding-bottom: 20px;\n}\n.wrapper button{\n  width: 30vw;\n  align-self: center;\n}\np {\n  font-size: 1.3em;\n}\n\n.field p {\n  font-size: 1.5em;\n  color: rgb(112, 110, 110);\n}\n\n.info {\n  display: flex;\n  flex-direction: column;\n  margin-bottom: 40px;\n}\n\n.profile img {\n  width: 56%;\n  height: 50%;\n\n}\n#pic {\n  border: 3px solid grey;\n  border-radius: 100%;\n}\n\n.profile {\n  width: 35%;\n  height: 470px;\n  border: 5px solid grey;\n  border-radius: 10px;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  padding: 3%;\n  margin-top: 0;\n}\n.profile p {\n  text-align: center;\n  word-wrap: break-word;\n}\n.usrInfo {\n  width: 50%;\n  height: 470px;\n  border: 5px solid grey;\n  border-radius: 10px;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: flex-start;\n  padding: 3%;\n  margin-left: 5px;\n}\n.field {\n  border-bottom: 1px solid rgb(212, 206, 206);\n  width: 100%;\n}\n\n\n\n</style>"],"sourceRoot":""}]}