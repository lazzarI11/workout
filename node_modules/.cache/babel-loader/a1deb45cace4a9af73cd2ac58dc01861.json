ArH;AAOC,GATT,EAOC,WAPD,C,iCA+BI,EApBI,KAAA,CAAA,O,kBADR,mBAAA,CASS,QATT,EASS;UAAA;AAPN,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA,EAAA;AAAc,MAAA,IAAA,CAAA,KAAA,CAAK,QAAL,EAAK;AAAA,QAAA,GAAA,EAAA,KAAA,CAAuB;AAAvB,OAAL;;AAA+C,MAAA,IAAA,CAAA,KAAA,CAAK,UAAL,EAAK,KAAL;;YAA0C,O,GAAO,K;KAA9G;AAOC,GATT,EAOC,QAPD,C,iCAqBI,EAVI,KAAA,CAAA,O,kBADR,mBAAA,CAUS,QAVT,EAUS;UAAA;AARN,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA,EAAA;AAAc,MAAA,IAAA,CAAA,KAAA,CAAK,UAAL,EAAK;AAAA,QAAA,GAAA,EAAA,KAAA,CAAyB;AAAzB,OAAL;;AAAiD,MAAA,IAAA,CAAA,KAAA,CAAK,UAAL,EAAK,KAAL;;YAA0C,O,GAAO,K;AAAqB,MAAA,QAAA,CAAA,UAAA;KAArI;AAQC,GAVT,EAQC,UARD,C,iCAWI,CAtCN,CAuCI,CA9EN,C","sourcesContent":["<template>\r\n  <!-- <Dialog :show=\"showDialog\"\r\n          :cancel=\"cancel\"\r\n          :confirm=\"$emit('deleteEx',{day:this.day})\"\r\n          title=\"Delete a day?\"\r\n          description=\"Are you sure you want to delete this day?\" /> -->\r\n\r\n  <div class=\"outerselect\">\r\n    <div v-if=\"!isAdded\" class=\"exercises\">\r\n      <div class=\"exCatSelect\">\r\n        <h2>Day {{ day }}</h2>\r\n\r\n        <div class=\"exercise\">\r\n          <span>Rest Day</span>\r\n          <input\r\n            type=\"checkbox\"\r\n            :value=\"true\"\r\n            v-model=\"isRestDay\"\r\n            @change=\"\r\n              restDaySelected();\r\n              $emit('emitSelectedEx', { day: this.day, arr: exerc });\r\n              $emit('dayAdded', true);\r\n            \"\r\n          />\r\n          <br />\r\n        </div>\r\n        <br />\r\n        <select\r\n          v-model=\"catValue\"\r\n          id=\"selectCat\"\r\n          @change=\"$emit('emitCategory', catValue)\"\r\n        >\r\n          <option value=\"\">Category</option>\r\n          <option value=\"3\">Chest</option>\r\n          <option value=\"1\">Abs</option>\r\n          <option value=\"2\">Legs</option>\r\n          <option value=\"4\">Arms</option>\r\n        </select>\r\n      </div>\r\n\r\n      <div class=\"exercise\" v-for=\"exercise in exercises\" :key=\"exercise.ex_id\">\r\n        {{ exercise.ex_name }}\r\n        <input type=\"checkbox\" :value=\"exercise\" v-model=\"exerc\" />\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"selected\">\r\n      <h4>Selected:</h4>\r\n\r\n      <div v-for=\"e in exerc\" :key=\"e.ex_id\">\r\n        {{ e.ex_name }}\r\n      </div>\r\n\r\n      <button\r\n        v-if=\"!isAdded\"\r\n        @click=\"\r\n          $emit('emitSelectedEx', { day: this.day, arr: exerc });\r\n          $emit('dayAdded', true);\r\n          added();\r\n        \"\r\n      >\r\n        Add day\r\n      </button>\r\n      <button\r\n        v-if=\"isAdded\"\r\n        @click=\"\r\n          $emit('editEx', { day: this.day });\r\n          $emit('dayAdded', false);\r\n          this.isAdded = false;\r\n        \"\r\n      >\r\n        Edit\r\n      </button>\r\n      <button\r\n        v-if=\"isAdded\"\r\n        @click=\"\r\n          $emit('deleteEx', { day: this.day });\r\n          $emit('dayAdded', false);\r\n          this.isAdded = false;\r\n          emptyExerc();\r\n        \"\r\n      >\r\n        Delete\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Dialog from \"./Dialog\";\r\nexport default {\r\n  components: { Dialog },\r\n\r\n  props: {\r\n    day: Number,\r\n    exercises: Array,\r\n    id: String,\r\n  },\r\n  emits: [\"emitCategory\", \"emitSelectedEx\", \"deleteEx\", \"editEx\", \"dayAdded\"],\r\n\r\n  data() {\r\n    return {\r\n      catValue: \"\",\r\n      exerc: [],\r\n      isAdded: false,\r\n      showDialog: false,\r\n      id: this.id,\r\n      day: this.day,\r\n      isRestDay: false,\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    exSelected() {\r\n      return this.exerc.length;\r\n    },\r\n  },\r\n\r\n  watch: {\r\n    exSelected(selectedEx) {\r\n      if (selectedEx > 4) {\r\n        alert(\"You can select maximum 4 exercises per day!\");\r\n        this.exerc.pop();\r\n      }\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    restDaySelected() {\r\n      if (this.isRestDay) {\r\n        this.exerc = [{ ex_id: 32, ex_name: \"REST DAY\" }];\r\n        this.isAdded = true;\r\n        this.isRestDay = false;\r\n        console.log(this.exerc);\r\n      }\r\n    },\r\n\r\n    added() {\r\n      if (this.exerc.length === 0) {\r\n        alert(\"You must select exercise!\");\r\n      } else {\r\n        this.isAdded = true;\r\n      }\r\n    },\r\n    emptyExerc() {\r\n      this.exerc = [];\r\n    },\r\n\r\n    cancel() {\r\n      console.log(\"cancel\");\r\n      this.showDialog = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ninput[type=\"checkbox\" i] {\r\n  margin: 10px 5px 1px 5px;\r\n  padding: 12px 0px 12px 0px;\r\n}\r\n.outerselect {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n.days {\r\n  display: flex;\r\n}\r\n.exercises {\r\n  /* max-height: 500px; */\r\n  width: 200px;\r\n  border: 3px solid #cd9f46;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n.exCatSelect {\r\n  margin: 10px;\r\n}\r\n.exCatSelect h2{\r\n  text-align: center;\r\n}\r\n.exCatSelect select{\r\n  display: block;\r\n  position: relative;\r\n  margin: auto;\r\n  margin-bottom: 0px;\r\n}\r\n\r\n.exercise {\r\n  min-height: 30px;\r\n  padding: 5px;\r\n  display: inline;\r\n  align-items: center;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  border-bottom: 1px solid #cd9f46;\r\n}\r\n\r\n.selected {\r\n  max-height: 500px;\r\n  width: 200px;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\nbutton {\r\n  \r\n  justify-content: center;\r\n  margin: auto;\r\n  margin-top: 10px;\r\n  border-radius: 7px;\r\n  width: 100px;\r\n  height: 30px;\r\n  font-size: 15px;\r\n  font-weight: bold;\r\n  border: 3px solid #cd9f46;\r\n}\r\n</style>"],"sourceRoot":""}]}